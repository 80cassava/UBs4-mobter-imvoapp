<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.terapanimvo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".PerusahaanDetailUlasanActivity"
            android:label="@string/title_ulasan"
            android:theme="@style/AppTheme.NoActionBar2" />
        <activity
            android:name=".PerusahaanDetailActivity"
            android:label="@string/title_dperusahaan"
            android:theme="@style/AppTheme.NoActionBar2" />
        <activity
            android:name=".PerusahaanActivity"
            android:label="@string/title_perusahaan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".JurusanDetailActivity"
            android:label="@string/title_djurusan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".JurusanActivity"
            android:label="@string/title_jurusan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>